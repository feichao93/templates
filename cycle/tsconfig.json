{
  "compilerOptions": {
    "module": "ES6",
    "moduleResolution": "node",
    "target": "es2017",
    "sourceMap": true,
    "jsx": "react",
    "jsxFactory": "Snabbdom.createElement",
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "allowJs": true,
    "noStrictGenericChecks": true
  },
  "exclude": ["node_modules"]
}
